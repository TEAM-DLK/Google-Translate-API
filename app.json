{
  "name": "Telegram Bot with DeepAI Integration",
  "description": "A Telegram bot that translates text and generates images using DeepAI.",
  "repository": "https://github.com/your-username/telegram-deepai-bot",
  "keywords": ["python", "telegram", "bot", "deepai", "image generation"],
  "env": {
    "TELEGRAM_BOT_TOKEN": {
      "description": "The token for your Telegram bot",
      "required": true
    },
    "OWNER_ID": {
      "description": "Telegram user ID for the bot owner",
      "required": true
    },
    "CHANNEL_LINK": {
      "description": "URL of the Telegram channel",
      "required": true
    },
    "GROUP_LINK": {
      "description": "URL of the Telegram group",
      "required": true
    },
    "DEEP_AI_API_KEY": {
      "description": "API key for DeepAI",
      "required": true
    }
  },
  "formation": {
    "worker": {
      "quantity": 1
    }
  },
  "scripts": {
    "postdeploy": "echo 'Deployment Successful!'"
  }
}